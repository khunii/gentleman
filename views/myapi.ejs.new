<!doctype html>
<html lang="en">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="/css/bootstrap.min.css">
    <link href="/css/signin.css" rel="stylesheet">
    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="/js/jquery-3.4.1.min.js"></script>
    <script src="/js/bootstrap.min.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
        });
    </script>
    <title>Gentleman</title>
</head>

<body>
    <div class="accordion form-signin" id="accordionApi">
        <% for(var i = 0; i < folder.length; i++){ %>
         <!-- 여기가 상위 item[i].name 순환시작 -->
         <div class="card">
            <div class="card-header" id="<%=i+"headingOne"%>">
                <h5 class="mb-0">
                    <div class="custom-control custom-checkbox">
                        <input type="checkbox" class="custom-control-input" id="<%=i+"upperCheck"%>">
                        <label class="custom-control-label" for=<%=i+"upperCheck"%>>
                            <button class="btn btn-link" type="button" data-toggle="collapse" data-target="<%="#"+i+"collapseOne"%>" aria-expanded="true" aria-controls="<%=i+"collapseOne"%>">
                                <%=folder[i].name%>
                            </button>
                        </label>
                    </div>
                </h5>
            </div>
    
            <div id="<%=i+"collapseOne"%>" class="collapse show" aria-labelledby="<%=i+"headingOne"%>" data-parent="#accordionApi">
                <div class="card-body">
                    <!-- 여기가 item[i].item[0~n]까지 반복시작 -->
                    <% for(var j =0 ; j < folder[i].item.length; j++){ %>
                        <div class="custom-control custom-checkbox ">
                        <input type="checkbox" class="custom-control-input" id="<%=i+"customCheck"+j%>">
                        <label class="custom-control-label" for="<%=i+"customCheck"+j%>"><%=folder[i].item[j].request.method%>&nbsp;<%=JSON.stringify(folder[i].item[j].request.url)%>&nbsp;<%=folder[i].item[j].name%></label>
                        <!-- <label class="custom-control-label" for="customCheck1">GET /fo/users getUserlist</label> -->
                        </div>
            <!-- 여기가 item[i].item[0~n]까지 반복종료 -->
                    <% } %>
                </div> 
            </div>
        </div>
        <!-- 여기가 상위 item[i].name 순환종료 -->           
        <% } %>
      </div>



    <script>
    </script>
</body>

</html>